<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <h1>Welcome to your profile!!!!</h1>
    <form action="/user/profile" method="post">
        <div class="form-group">
            <label for="eventName">Name of event</label>
            <input type="text" id="eventName" name="eventName" class="form-control">
        </div>
        <div class="form-group">
            <label for="day">Day(19.03.2019)</label>
            <input type="text" id="day" name="day" class="form-control">
        </div>
        <div class="form-group">
            <label for="time">Time(22:00)</label>
            <input type="text" id="time" name="time" class="form-control">
        </div>
        <div class="form-group">
            <label for="data">Enter description of your event</label>
            <input type="text" id="data" name="data" class="form-control">
        </div>
        <input type="hidden" name="_csrf" value="">
        <button type="submit" class="btn btn-primary">Add action</button>
    </form>
    <form action="/user/event" method="get">
      <button type="submit" class="btn btn-primary">See event</button>
    </form>
  </div>
</div>
